{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Authentication": {
    "JwtBearer": {
      "IsEnabled": "true",
      "Issuer": "App",
      "Audience": "AppUser",
      "ExpireInMinutes": 20
    }
  },
  "AllowedHosts": "*"
}
